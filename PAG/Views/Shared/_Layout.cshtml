                                <img src="@Url.Action("imageUser", "Home")" alt="admin">
                                <i class="online"></i>
                            </a>
                            <div class="media-body">
                                <h4 class="media-heading">Hola, <span>@decoratorSession.Claims.NOMBRES@*@(((SefinClaims.UsuarioClaim)System.Web.HttpContext.Current.Session["Claims"]).NOMBRES.ToString())*@</span></h4>
                                <small>@decoratorSession.Claims.PUESTO@*@(((SefinClaims.UsuarioClaim)System.Web.HttpContext.Current.Session["Claims"]).PUESTO)*@</small>
                            </div>
                        </div>
                        using (Html.BeginForm("setPerfil", "Home", new { retornoUrl = Html.Raw(Request.Url.AbsoluteUri) }, FormMethod.Post, new { id = "formGlobal" }))
                        {
                            {
                                if (decoratorSession.Permisos.Perfiles.Count() > 0)
                                {
                                    var idPerfil = decoratorSession.Permisos.PerfilActual;
                                    var nombrePerfil = decoratorSession.Permisos.Perfiles.First(x => x.ID_PERFIL == idPerfil).NOMBRE_PERFIL;

                                    <input type="hidden" id="ID_PERFIL" value="@idPerfil" name="ID_PERFIL" />

                                    <div class="btn-group btn-block">
                                        <button type="button" class="btn btn-sm dropdown-toggle btn-block" data-toggle="dropdown" aria-expanded="false">
                                            <span class="any">@(nombrePerfil.Length > 25 ? nombrePerfil.Substring(0, 24) : nombrePerfil )</span><span class="anysmall">...</span> <span class="caret"></span>
                                        </button>
                                        <ul class="dropdown-menu" role="menu">
                                            <li><a href="javascript:void(0);">Seleccione Perfil:</a></li>
                                            <li class="divider"></li>
                                            @foreach (var x in decoratorSession.Permisos.Perfiles)
                                            {
                                                if (idPerfil == x.ID_PERFIL)
                                                {
                                                    <li class="list-group-item-info"><a href="javascript: submitform('APP-IDPERFIL',@x.ID_PERFIL);" class="submit-link" data-idperfil="@x.ID_PERFIL">@x.NOMBRE_PERFIL</a></li>

                                                }
                                                else
                                                {
                                                    <li><a href="javascript: submitform('APP-IDPERFIL',@x.ID_PERFIL);" class="submit-link" data-idperfil="@x.ID_PERFIL">@x.NOMBRE_PERFIL</a></li>
                                                }
                                            }

                                        </ul>
                                    </div>

                                }// END IF (decoratorSession.Permisos.Perfiles.Count() > 0)

                                <br />

                                var flujo = (string)System.Web.HttpContext.Current.Session["ID_FLUJO"];
                                if (!string.IsNullOrEmpty(flujo) && flujo != "0")
                                {


                                    if (((SAS_DTO.AUX_SEGURIDAD_DTO)System.Web.HttpContext.Current.Session["Permisos"]).FlujosOperaciones.Count() > 0)
                                    {
                                        var idOperacion = ((string)System.Web.HttpContext.Current.Session["ID_OPERACION"]) == null ? ((SAS_DTO.AUX_SEGURIDAD_DTO)System.Web.HttpContext.Current.Session["Permisos"]).FlujosOperaciones.FirstOrDefault().ID_OPERACION : System.Web.HttpContext.Current.Session["ID_OPERACION"];
                                        var nombreOperacion = ((SAS_DTO.AUX_SEGURIDAD_DTO)System.Web.HttpContext.Current.Session["Permisos"]).FlujosOperaciones.First(x => x.ID_OPERACION.ToString() == idOperacion.ToString()).OPERACION;

                                        <input type="hidden" id="ID_OPERACION" value="@idOperacion" name="ID_OPERACION" />

                                        <div class="btn-group btn-block">
                                            <button type="button" class="btn btn-sm dropdown-toggle btn-block" data-toggle="dropdown" aria-expanded="false">
                                                <span class="any">@nombreOperacion.Substring(0, 24)</span><span class="anysmall">...</span> <span class="caret"></span>
                                            </button>
                                            <ul class="dropdown-menu" role="menu">
                                                <li><a href="javascript:void(0);">Seleccione Operación:</a></li>
                                                <li class="divider"></li>
                                                @foreach (var x in ((SAS_DTO.AUX_SEGURIDAD_DTO)System.Web.HttpContext.Current.Session["Permisos"]).FlujosOperaciones)
                                                {
                                                    if (idOperacion.ToString() == x.ID_OPERACION.ToString())
                                                    {
                                                        <li class="list-group-item-info"><a href="javascript: submitform('APP-IDOPERACION',@x.ID_OPERACION);" class="submit-link" data-idoperacion="@x.ID_OPERACION">@x.OPERACION</a></li>

                                                    }
                                                    else
                                                    {
                                                        <li><a href="javascript: submitform('APP-IDOPERACION',@x.ID_OPERACION);" class="submit-link" data-idoperacion="@x.ID_OPERACION">@x.OPERACION</a></li>
