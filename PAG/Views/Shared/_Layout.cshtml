        var modal = $(this);

        modal.find('#layoutModalTitulo').text(recipient);

        //modal.find('.modal-body input').val(recipient)
    });

    function submitform(global, args) {
        if (global == "APP-IDPERFIL") {
            $("#ID_PERFIL").val(args);
        }
        if (global == "APP-IDOPERACION") {
            $("#ID_OPERACION").val(args);
        }
        $(document.getElementById('formGlobal')).closest('form').submit();
    }

    function DropDownClick() {
        $("#APP_ID_GESTION").change(function () {
            var sel = this.value;
            var urlReturn = '@Html.Raw(Request.Url.AbsoluteUri)';
            var urlGestion = '@Url.Action("setGestion", "Home")';
            CambioGestion(sel, urlGestion, urlReturn);
        });
